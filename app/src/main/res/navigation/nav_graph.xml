<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/homeFragment"
        android:name="com.umc.sculptor.ui.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_museumProfileMeFragment"
            app:destination="@id/museumProfileMeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_storeFragment"
            app:destination="@id/storeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_museumProfileOtherFragment"
            app:destination="@id/museumProfileOtherFragment" />
        <action
            android:id="@+id/action_homeFragment_to_workshopFragment"
            app:destination="@id/workshopFragment" />
        <action
            android:id="@+id/action_homeFragment_to_alarmFragment"
            app:destination="@id/alarmFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_friendStoneFragment"
            app:destination="@id/friendStoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeFragment"
        android:name="com.umc.sculptor.ui.store.StoreFragment"
        android:label="StoreFragment" >
        <action
            android:id="@+id/action_storeFragment_to_itemListFragment"
            app:destination="@id/itemListFragment" />
    </fragment>
    <fragment
        android:id="@+id/workshopFragment"
        android:name="com.umc.sculptor.ui.workshop.WorkshopFragment"
        android:label="WorkshopFragment"
        >
        <action
            android:id="@+id/action_workshopFragment_to_detailWorkshopFragment"
            app:destination="@id/detailWorkshopFragment" />
        <action
            android:id="@+id/action_workshopFragment_to_rockNameFragment"
            app:destination="@id/rockNameFragment" />
    </fragment>
    <fragment
        android:id="@+id/museumProfileMeFragment"
        android:name="com.umc.sculptor.ui.museum.MuseumProfileMeFragment"
        android:label="MuseumProfileMeFragment" >
        <action
            android:id="@+id/action_museumProfileMeFragment_to_museumEditFragment"
            app:destination="@id/museumEditFragment" />
        <action
            android:id="@+id/action_museumProfileMeFragment_to_museumSculptorFragment"
            app:destination="@id/museumSculptorFragment" />
    </fragment>
    <fragment
        android:id="@+id/museumProfileOtherFragment"
        android:name="com.umc.sculptor.ui.museum.MuseumProfileOtherFragment"
        android:label="MuseumProfileOtherFragment" >
        <action
            android:id="@+id/action_museumProfileOtherFragment_to_museumSculptorFragment"
            app:destination="@id/museumSculptorFragment" />
    </fragment>
    <fragment
        android:id="@+id/museumEditFragment"
        android:name="com.umc.sculptor.ui.museum.MuseumEditFragment"
        android:label="fragment_museum_edit"
        tools:layout="@layout/fragment_museum_edit" >
        <action
            android:id="@+id/action_museumEditFragment_to_museumProfileMeFragment"
            app:destination="@id/museumProfileMeFragment" />
    </fragment>
    <fragment
        android:id="@+id/museumSculptorFragment"
        android:name="com.umc.sculptor.ui.museum.MuseumSculptorFragment"
        android:label="fragment_museum_sculptor"
        tools:layout="@layout/fragment_museum_sculptor" >
        <action
            android:id="@+id/action_museumSculptorFragment_to_museumProfileMeFragment"
            app:destination="@id/museumProfileMeFragment" />
    </fragment>
    <fragment
        android:id="@+id/alarmFragment"
        android:name="com.umc.sculptor.ui.home.AlarmFragment"
        android:label="AlarmFragment" />
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.umc.sculptor.ui.home.MyPageFragment"
        android:label="MyPageFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.umc.sculptor.ui.home.SearchFragment"
        android:label="SearchFragment" >
        <action
            android:id="@+id/action_searchFragment_to_museumProfileOtherFragment"
            app:destination="@id/museumProfileOtherFragment" />
        <action
            android:id="@+id/action_searchFragment_to_friendStoneFragment"
            app:destination="@id/friendStoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/rockCategoryFragment"
        android:name="com.umc.sculptor.ui.workshop.createStone.RockCategoryFragment"
        android:label="RockCategoryFragment" >
        <action
            android:id="@+id/action_rockCategoryFragment_to_rockGoalFragment"
            app:destination="@id/rockGoalFragment" />
    </fragment>
    <fragment
        android:id="@+id/rockGoalFragment"
        android:name="com.umc.sculptor.ui.workshop.createStone.RockGoalFragment"
        android:label="RockGoalFragment" >
        <action
            android:id="@+id/action_rockGoalFragment_to_rockDateFragment"
            app:destination="@id/rockDateFragment" />
    </fragment>
    <fragment
        android:id="@+id/rockDateFragment"
        android:name="com.umc.sculptor.ui.workshop.createStone.RockDateFragment"
        android:label="RockDateFragment" >
        <action
            android:id="@+id/action_rockDateFragment_to_rockCompleteFragment"
            app:destination="@id/rockCompleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/rockCompleteFragment"
        android:name="com.umc.sculptor.ui.workshop.createStone.RockCompleteFragment"
        android:label="RockCompleteFragment" >
        <action
            android:id="@+id/action_rockCompleteFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/rockNameFragment"
        android:name="com.umc.sculptor.ui.workshop.createStone.RockNameFragment"
        android:label="RockNameFragment" >
        <action
            android:id="@+id/action_rockNameFragment_to_rockCategoryFragment"
            app:destination="@id/rockCategoryFragment" />
    </fragment>

    <fragment
        android:id="@+id/itemListFragment"
        android:name="com.umc.sculptor.ui.store.ItemListFragment"
        android:label="ItemListFragment" >
        <action
            android:id="@+id/action_itemListFragment_to_storeFragment"
            app:destination="@id/storeFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailWorkshopFragment"
        android:name="com.umc.sculptor.ui.workshop.DetailWorkshopFragment"
        android:label="DetailWorkshopFragment" >
        <action
            android:id="@+id/action_detailWorkshopFragment_to_todaycheckFragment"
            app:destination="@id/todaycheckFragment" />
    </fragment>
    <fragment
        android:id="@+id/todaycheckFragment"
        android:name="com.umc.sculptor.ui.workshop.TodaycheckFragment"
        android:label="TodaycheckFragment" >
        <action
            android:id="@+id/action_todaycheckFragment_to_stonepowderFragment"
            app:destination="@id/stonepowderFragment" />
        <action
            android:id="@+id/action_todaycheckFragment_to_workshopFragment"
            app:destination="@id/workshopFragment" />
    </fragment>
    <fragment
        android:id="@+id/stonepowderFragment"
        android:name="com.umc.sculptor.ui.workshop.StonepowderFragment"
        android:label="StonepowderFragment" >
        <action
            android:id="@+id/action_stonepowderFragment_to_workshopFragment"
            app:destination="@id/workshopFragment" />
    </fragment>
    <fragment
        android:id="@+id/friendStoneFragment"
        android:name="com.umc.sculptor.ui.home.FriendStoneFragment"
        android:label="FriendStoneFragment" >
        <action
            android:id="@+id/action_friendStoneFragment_to_museumProfileOtherFragment"
            app:destination="@id/museumProfileOtherFragment" />
    </fragment>
</navigation>